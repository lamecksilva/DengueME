      <field minimum="-1e+06" maximum="1e+06" precision="3" type="doublespinbox" id="infection" step="0.001" label="Infected">0.000</field>
      <field minimum="-1e+06" maximum="1e+06" precision="3" type="doublespinbox" id="rv" step="0.001" label="Recovered">0.000</field>
