<xmlmodel>
  <script interpreter="">
    <file>Temporal.lua</file>
    <main>Temporal.lua</main>
  </script>
  <parameters>
    <groupbox label="Parameters">
      <field minimum="-1e+06" maximum="1e+06" type="spinbox" id="ODE" label="ODE steps:">0</field>
      <field minimum="-1000000.000000000" maximum="1000000.000000000" precision="9" type="doublespinbox" id="K" step="0.000000001" label="Carrying capacity (K):">0.000000000</field>
      <csv id="Temp" label="Temperature time series"></csv>
      <csv id="AmostraOvos" label="Eggs sample"></csv>
    </groupbox>
    <groupbox label="Initial values">
      <field minimum="0" maximum="1000000" type="spinbox" id="INIT_EGGS" label="Eggs:">0</field>
      <field minimum="0" maximum="1000000" type="spinbox" id="INIT_LARVAE" label="Larvae:">0</field>
      <field minimum="0" maximum="1000000" type="spinbox" id="INIT_PUPAE" label="Pupae:">0</field>
      <field minimum="0" maximum="1000000" type="spinbox" id="INIT_ADULTS" label="Adults:">0</field>
    </groupbox>
    <groupbox label="Mortality rate">
      <field minimum="-1000000.000000" maximum="1000000.000000" precision="6" type="doublespinbox" id="M1" step="0.000001" label="Egg:">0.000000</field>
      <field minimum="-1000000.000000" maximum="1000000.000000" precision="6" type="doublespinbox" id="M2" step="0.000001" label="Larvae:">0.000000</field>
      <field minimum="-1000000.000000" maximum="1000000.000000" precision="6" type="doublespinbox" id="M3" step="0.000001" label="Pupae:">0.000000</field>
      <field minimum="-1000000.000000" maximum="1000000.000000" precision="6" type="doublespinbox" id="M4" step="0.000001" label="Mosquito:">0.000000</field>
    </groupbox>
    <groupbox label="Development rate">
      <field minimum="-1000000.000000" maximum="1000000.000000" precision="6" type="doublespinbox" id="txTransO" step="0.000001" label="Egg:">0.000000</field>
      <field minimum="-1000000.000000" maximum="1000000.000000" precision="6" type="doublespinbox" id="txTransL" step="0.000001" label="Larvae:">0.000000</field>
      <field minimum="-1000000.000000" maximum="1000000.000000" precision="6" type="doublespinbox" id="txTransP" step="0.000001" label="Pupae:">0.000000</field>
    </groupbox>
  </parameters>
  <simulation label="Simulation">
    <field type="checkbox" id="DEBUG_MODE" name="Debug mode" label="">false</field>
    <field minimum="0" maximum="1000000000" type="spinbox" id="SIMULATION_TIME_START" label="Simulation start time:">0</field>
    <field minimum="0" maximum="1000000000" type="spinbox" id="SIMULATION_TIME_END" label="Simulation end time:">0</field>
    <field minimum="0" maximum="1000000000" type="spinbox" id="SIMULATION_TIME_START_LOG" label="Log start time:">0</field>
    <field minimum="0" maximum="1000000000" type="spinbox" id="SIMULATION_TIME_END_LOG" label="Log end time:">0</field>
  </simulation>
  <results label="Results">
    <field type="lineedit" id="RESULTS_DIR" label="Directory:">coupled_results</field>
    <field type="lineedit" id="RESULTS_FILE" label="File:">results.csv</field>
  </results>
</xmlmodel>
